{
  "name": "pret_joy",
  "version": "1.0.0",
  "description": "MUI Joy plugin for Pret",
  "main": "client/index.js",
  "repository": "https://github.com/percevalw/pret",
  "author": "Perceval Wajsb√ºrt <perceval.wajsburt@gmail.com>",
  "scripts": {
    "build": "jlpm clean && jupyter labextension build .",
